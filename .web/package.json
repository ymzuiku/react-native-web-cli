{
  "name": "sub-web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "to": "bash scripts/starter.sh",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx,js,jsx}'",
    "clear": "rm -rf yarn.lock && watchman watch-del-all && rm -rf node_modules && yarn",
    "test": "jest"
  },
  "devDependencies": {
    "@types/history": "^4.7.0",
    "@types/lodash": "^4.14.116",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-native": "^0.56.9",
    "@types/react-navigation": "^2.0.16",
    "@types/react-redux": "^6.0.6",
    "@types/react-router": "^4.0.30",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.6",
    "autoprefixer": "^9.1.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "23.4.2",
    "babel-plugin-react-native-web": "^0.8.9",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "^5.0.2",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "jest": "23.5.0",
    "postcss-modules": "^1.3.2",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.4.1",
    "rm": "^0.1.8",
    "ts-jest": "^23.1.3",
    "typescript": "^3.0.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "lodash": "^4.17.10",
    "react": "^16.4.2",
    "react-art": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-native": "^0.56.0",
    "react-native-web": "^0.8.9",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  }
}
