{
  "name": "react-native-web-cli",
  "version": "0.1.0",
  "private": true,
  "dll": {
    "any": [
      "babel-polyfill",
      "react",
      "react-dom",
      "react-native-web"
    ],
    "src": [
      "react-motion",
      "react-native-tab-view",
      "react-native-device-info",
      "redux",
      "react-redux",
      "redux-thunk",
      "redux-logger",
      "immutable",
      "lodash"
    ]
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios-x": "react-native run-ios --simulator \"iPhone X\"",
    "web": "webpack-dev-server --config webpack/webpack.dev.js",
    "web-open": "webpack-dev-server --config webpack/webpack.dev.js --open",
    "web-build": "webpack --progress --config webpack/webpack.prod.js",
    "web-dll": "webpack --progress --config webpack/webpack.dll.js",
    "test": "jest"
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "immutable": "^3.8.2",
