    "react-art": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-motion": "^0.5.2",
    "react-native": "~0.55.2",
    "react-native-device-info": "^0.21.5",
    "react-native-image-picker": "^0.26.10",
    "react-native-tab-view": "^1.0.2",
    "react-native-web": "^0.8.3",
    "react-navigation": "^2.3.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
